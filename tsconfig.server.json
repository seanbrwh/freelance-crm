{
  "extends": "./tsconfig.json",
  "include": ["./src/Server/**/**/*"],
  "exclude": ["./src/App/**/**/*"],
  "compilerOptions": {
    "outDir": "./dist",
    "noEmit": false,
    "module": "commonjs",
    "target": "ES6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true
  }
}
