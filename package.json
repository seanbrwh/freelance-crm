{
  "name": "freelancecrm",
  "description": "crm for freelancers",
  "author": "Sean White (seanbrwh@gmail.com)",
  "license": "MIT",
  "scripts": {
    "start:fix": "docker-compose down && docker-compose up --build && yarn install",
    "dev": "concurrently -n w: yarn:dev:*",
    "dev:tasks": "gulp",
    "dev:app": "webpack-dev-server --mode development --env.LOCAL=true --env.DOCKER=false --env.PUBLIC_PATH=http://localhost:3015",
    "dev:server": "tsc-watch --p \"./tsconfig.server.json\" --onSuccess \"nodemon ./dist/index.js\"",
    "docker:dev": "docker-compose down && docker-compose up --build",
    "app:analyze": "webpack --mode=production --env.analyze=true",
    "typescript": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.1",
    "dotenv": "^8.2.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "gulp": "^4.0.2",
    "nodemon": "^2.0.2",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {
    "@types/react-router": "^5.1.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "gulp-rename": "^2.0.0",
    "morgan": "^1.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  }
}
